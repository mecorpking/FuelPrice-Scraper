 // The IdentityUpdated event happens when a UID2 token is generated or refreshed.\n          // payload.identity contains the resulting latest identity.\n          break;\n      }\n    });\n}*/\nif(document.querySelector(\".oi-user\")){\n    var login_id = '';\n    if (/Mobi|Android/i.test(navigator.userAgent)) {\n        var device = 'mobile';\n    }else{\n        var device = 'desktop';\n    }\n    var htmlLang = document.documentElement.lang || 'en';\n    var storedUserId = getCookieCommon('MSLUserId');\n    var storedUserDataJson = getCookieCommon('MSLUserData');\n    var onetaplogin=0;\n    if (storedUserId) {\n        login_id = storedUserId;\n        if(storedUserDataJson){\n            displayUserData(storedUserDataJson);\n            displayProfileDetails(storedUserDataJson);\n        }else{\n            displayProfileDetailsFromDB(login_id);\n        }\n        \n        var isLoggedin=1;\n        if(document.querySelector(\".oi-user\")){\n            document.querySelector(\".oi-user\").classList.add(\"loggedin\");\n        }\n    }else{\n       var isLoggedin=0;\n        if(document.querySelector(\".oi-user\")){\n            document.querySelector(\".oi-user\").classList.remove(\"loggedin\");\n        }\n    }\n\tfunction startGoogleSignIn() {\n        const originalHost = window.location.host; // Capture the original host\n        const stateData = {originalHost,device};\n        const stateEncoded = encodeURIComponent(JSON.stringify(stateData));\n        const clientId = '970843911652-n5m0f1deug4nu0f0la67l5e9f4hifl4h.apps.googleusercontent.com';\n        const redirectUri = 'https://users.oneindia.com/account/cms/userauth.html';\n        const authorizationUrl = `https://accounts.google.com/o/oauth2/auth?client_id=${clientId}&redirect_uri=${encodeURIComponent(redirectUri)}&response_type=code&scope=email openid profile&prompt=consent&state=${stateEncoded}`;\n        const popup = window.open(authorizationUrl, 'GoogleSignInPopup', 'width=500,height=600');\n}\nwindow.addEventListener('message', (event) => {\n    const allowedDomains = ['https://users.oneindia.com'];\n    if (allowedDomains.includes(event.origin) && event.data.type === 'login') {\n        console.log(event.data);\n        var loginStatus = event.data.status;\n      const userdata = event.data.code.user_info;\n      if(loginStatus){\n        var authToken = userdata['access_token'];\n        login_id=userdata['id'];\n        var givenname = userdata['given_name'];\n        if(document.querySelector(\".oi-registration-popup\")){\n            document.querySelector('.oi-registration-popup').classList.remove('login');\n        }\n        if(document.querySelector(\".oi-registration-popup-overlay\")){\n            document.querySelector('.oi-registration-popup-overlay').classList.remove('login');\n        }   \n        //document.body.style.overflow = \"auto\";\n        if(document.querySelector(\".oi-user\")){\n            document.querySelector(\".oi-user\").classList.add(\"loggedin\");\n        }\n        displayUserData(userdata);\n      }else{\n        //document.body.style.overflow = \"auto\";\n        if(document.querySelector(\".oi-user\")){\n            document.querySelector(\".oi-user\").classList.remove(\"loggedin\");\n        }\n      }\n    }\n  });\nfunction capitalizeFirstLetter(string) {\n    return string.charAt(0).toUpperCase() + string.slice(1);\n}\n\nfunction displayProfileDetails(userFields) {\n        var responsePayload = JSON.parse(userFields);\n        if (document.querySelector(\"#firstname\")) {\n            const firstNameElement = document.querySelector(\"#firstname\");\n            const firstNameText = responsePayload.given_name ?? 'NA';\n            const capitalizedFirstName = capitalizeFirstLetter(firstNameText);\n            firstNameElement.innerHTML = capitalizedFirstName;\n        }\n        if (document.querySelector(\"#lastname\")) {\n            const lastNameElement = document.querySelector(\"#lastname\");\n            const lastNameText = responsePayload.family_name ?? 'NA';\n            const capitalizedLastName = capitalizeFirstLetter(lastNameText);\n            lastNameElement.innerHTML = capitalizedLastName;\n        }\n        if(document.querySelector(\"#emailaddress\")){\n            document.querySelector(\"#emailaddress\").innerHTML=responsePayload.email ?? 'NA';\n        }\n        if(document.querySelector(\"#cityname\")){\n            document.querySelector(\"#cityname\").innerHTML=responsePayload.city ?? 'NA';\n        }\n        if(document.querySelector(\".selectage\")){\n            document.querySelector(\".selectage\").innerHTML=responsePayload.age || 'Select your Age Range';\n        }\n        if(document.querySelector(\".selectgender\")){\n            var genderselected = responsePayload?.gender || 'Select your Gender';\n            document.querySelector(\".selectgender\").innerHTML=capitalizeFirstLetter(genderselected);\n        }\n        if (document.querySelector('.oi-myprofile-genderlist')) {\n            var genderval = responsePayload.gender ?? 'NA';\n            const genderListItems = document.querySelectorAll('.oi-myprofile-genderlist li');\n            const selectedGender = genderval;\n            if(genderval=='NA'){\n                genderListItems.forEach(function(li) {\n                    li.classList.remove('active');\n                });\n            }else{\n                genderListItems.forEach(function(li) {\n                const genderAttributeValue = li.getAttribute('data-gender');\n                    if (genderAttributeValue === selectedGender) {\n                        li.classList.add('active');\n                    }\n                });\n            }\n        }\n        if (!responsePayload.login_token) {\n            var logid = responsePayload.id;\n            if(logid){\n                var apiUrl = 'https://users.oneindia.com/account/cms/userAuthentication.php?type=getUserKey&host='+host1+'&device='+device+'&login_id='+logid;\n                fetch(apiUrl, {\n                    method: 'GET',\n                })\n                .then(response => response.json())\n                .then(data => {\n                    if(data['msg']==1){\n                        var finalData = data['result'];\n                        responsePayload.login_token = finalData;\n                        var finalLocalData = JSON.stringify(responsePayload);\n                        setCookieCommon(\"MSLUserData\", finalLocalData, 10000000,'/','');\n                    }\n                })\n                .catch(error => {\n                    // Handle errors\n                    console.error('Error:', error);\n                });\n            }\n        }\n        if(document.getElementById('profileicon')){\n            document.getElementById('profileicon').style.display = 'none';\n        }\n        if(document.getElementById('profileiconactive')){\n           document.getElementById('profileiconactive').style.display = 'inline-block';\n        }\n        /*if(host1=='www.oneindia.com'){\n            if(responsePayload.email){\n                uid2Integration(responsePayload.email);\n            }\n        }*/\n    }\nfunction inertUserDatainDB(userData) {\n    const storedUserLocDataJson = localStorage.getItem('MSLUserLoc');\n    if (storedUserLocDataJson) {\n        const storedUserData = JSON.parse(storedUserLocDataJson);\n        const userDataObject = JSON.parse(userData);\n        userDataObject.city = storedUserData.city ?? 'NA';\n        userDataObject.ip = storedUserData.ip ?? 'NA';\n        userDataObject.state = storedUserData.State ?? 'NA';\n        userDataObject.country = storedUserData.country ?? 'NA';\n        userData = JSON.stringify(userDataObject);\n    }\n    var apiUrl = '/scripts/cms/controller.php?module=subscription&lang_type=' + htmlLang + '&device='+device+'&type=insertUserDetails';\n    fetch(apiUrl, {\n        method: 'POST',\n        headers: {\n            'Content-Type': 'application/json'\n        },\n        body: userData\n    })\n        .then(response => response.json())\n        .then(data => {\n            console.log(data);\n            if(data && data['msg']=='updated'){\n                \n                if(document.querySelector(\".selectgender\")){\n                    var selgender = data['gender'] || 'Select your Gender';\n                    document.querySelector(\".selectgender\").innerHTML=capitalizeFirstLetter(selgender);\n                }\n                if (document.querySelector('.oi-myprofile-genderlist')) {\n                    var genderval = data['gender'] ?? 'NA';\n                    setTimeout(function() {\n                        updateLocalStorage('gender',data['gender']);\n                    },1000)\n                    const genderListItems = document.querySelectorAll('.oi-myprofile-genderlist li');\n                    const selectedGender = genderval;\n                    if(genderval=='NA'){\n                        genderListItems.forEach(function(li) {\n                            li.classList.remove('active');\n                        });\n                    }else{\n                        genderListItems.forEach(function(li) {\n                        const genderAttributeValue = li.getAttribute('data-gender');\n                            if (genderAttributeValue === selectedGender) {\n                                li.classList.add('active');\n                            }\n                        });\n                    }\n                }\n                if(document.querySelector(\".selectage\")){\n                    document.querySelector(\".selectage\").innerHTML=data['age'] ?? 'Select your Age Range';\n                    updateLocalStorage('age',data['age']);\n                }\n            }\n        })\n        .catch(error => {\n            // Handle errors\n            console.error('Error:', error);\n        });\n}\nfunction displayProfileDetailsFromDB(login_id) {\n        var apiUrl = 'https://users.oneindia.com/account/cms/userAuthentication.php?type=fetchProfileDetails&device='+device+'&login_id='+login_id+'&host='+host1;\n        fetch(apiUrl, {\n            method: 'GET',\n        })\n        .then(response => response.json())\n        .then(data => {\n            if(data['msg']==1){\n                var finalData = data['result'];\n                var finalLocalData = JSON.stringify(finalData);\n                displayProfileDetails(finalLocalData);\n                displayUserData(finalLocalData);\n                setCookieCommon(\"MSLUserData\", finalLocalData, 10000000,'/','');\n            }\n        })\n        .catch(error => {\n            // Handle errors\n            console.error('Error:', error);\n        });\n}\nfunction logoutUserFromDB(login_id) {\n    login_id = getCookieCommon('MSLUserId');\n    var apiUrl = 'https://users.oneindia.com/account/cms/userAuthentication.php?type=logoutUser&device='+device+'&login_id='+login_id+'&host='+host1;\n        fetch(apiUrl, {\n            method: 'GET',\n        })\n        .then(response => response.json())\n        .then(data => {\n            if(data['msg']==1){\n                deleteCookieCommon(\"MSLUserId\");\n                deleteCookieCommon(\"MSLUserData\");\n                /*if(window.__uid2){\n                    window.__uid2.disconnect();\n                }*/\n                isLoggedin=0;\n            }\n        })\n        .catch(error => {\n            console.error('Error:', error);\n        });\n}\nfunction updateLocalStorage(key, value) {\n    var storedUserDataJson = getCookieCommon('MSLUserData');\n    if (storedUserDataJson) {\n        var data = JSON.parse(storedUserDataJson);\n        data[key] = value;\n        var updatedJsonString = JSON.stringify(data);\n        setCookieCommon(\"MSLUserData\", updatedJsonString, 10000000,'/','');\n    }\n}\nfunction updateAgeGender(login_id,fld,val) {\n    var apiUrl = 'https://users.oneindia.com/account/cms/userAuthentication.php?type=updateAgeGender&field='+fld+'&value='+val+'&device='+device+'&login_id='+login_id+'&host='+host1;\n        fetch(apiUrl, {\n            method: 'GET',\n        })\n        .then(response => response.json())\n        .then(data => {\n            updateLocalStorage(fld,val);\n        })\n        .catch(error => {\n            // Handle errors\n            console.error('Error:', error);\n        });\n}\nfunction displayUserData(userFields) {\n        if(isLoggedin==0){\n            var gid = userFields.id;\n            var adata = JSON.stringify(userFields);\n            setCookieCommon(\"MSLUserId\", gid, 10000000,'/','');\n            setCookieCommon(\"MSLUserData\", adata, 10000000,'/','');\n            isLoggedin=1;\n            displayProfileDetails(adata);\n            //inertUserDatainDB(adata);\n        }\n    }\nfunction handleCredentialResponse(response) {\n            var customDataObj={};\n            const originalHost = window.location.host;\n            var credential = response.credential;\n             var code=credential;\n             var apiurl = 'https://users.oneindia.com/account/cms/userAuthentication.php?type=onetaptokenExchange&code='+code+'&state='+originalHost+'&device='+device;\n            if(code){\n                fetch(apiurl, {\n                    method: 'GET',\n                })\n                .then(response => response.json())\n                .then(data => {\n                        if(data){\n                          const userdata = data.user_info;\n                          if(userdata!='NA'){\n                            onetaplogin=1;\n                            if(document.querySelector(\".oi-user\")){\n                                document.querySelector(\".oi-user\").classList.add(\"loggedin\");\n                            }\n                              customDataObj.access_token='NA';\n                              customDataObj.email=userdata['email'];\n                              customDataObj.family_name=userdata['family_name'];\n                              customDataObj.given_name=userdata['given_name'];\n                              customDataObj.id=userdata['sub'];\n                              customDataObj.name=userdata['name'];\n                              customDataObj.picture=userdata['picture'];\n                              customDataObj.verified_email=userdata['verified_email'];\n                              displayUserData(customDataObj);\n                            }else{\n                              if(document.querySelector(\".oi-user\")){\n                                    document.querySelector(\".oi-user\").classList.remove(\"loggedin\");\n                                }\n                            }\n                        }else{\n                          if(document.querySelector(\".oi-user\")){\n                                    document.querySelector(\".oi-user\").classList.remove(\"loggedin\");\n                                }\n                        }\n                        \n                })\n                .catch(error => {\n                    console.log('Token exchange error:', error);\n                    if(document.querySelector(\".oi-user\")){\n                                    document.querySelector(\".oi-user\").classList.remove(\"loggedin\");\n                                }\n                });\n            }\n        }\nvar checkGoogleAccountsInterval;\nfunction loadGoogleIdentityLibrary(callback) {\n    var script = document.createElement('script');\n    script.src = 'https://accounts.google.com/gsi/client';\n    script.async = true;\n    script.defer = true;\n    script.onload = function() {\n        console.log('Google Identity Services library loaded successfully.');\n        initializeGoogleSignIn(); // Once loaded, try to initialize Google Sign-In\n    };\n\n    script.onerror = function() {\n        console.error('Error loading Google Identity Services library.');\n    };\n\n    document.head.appendChild(script);\n}\n\nfunction initializeGoogleSignIn() {\n    if (google.accounts && google.accounts.id && google.accounts.id.initialize) {\n        google.accounts.id.initialize({\n            client_id: '970843911652-n5m0f1deug4nu0f0la67l5e9f4hifl4h.apps.googleusercontent.com',\n            callback: handleCredentialResponse,\n        });\n        google.accounts.id.prompt();\n        clearInterval(checkGoogleAccountsInterval); // Clear the interval once initialized\n        setCookieCommon(\"onetappopup\",1,24,'/','');\n    } else {\n        console.log('Google Identity Services not yet available. Retrying...');\n    }\n}\nvar onetapPopup = getCookieCommon('onetappopup');\nif(onetapPopup){\n    //One Tap Popup already shown\n}else{\n  var isLoginPopup = false;\n    window.addEventListener(\"scroll\", function () {\n      amountscrolled1();\n      if(pctScrolled1>30){\n        if(isLoggedin==0){\n        if (!isLoginPopup) {\n          isLoginPopup = true;\n           checkGoogleAccountsInterval = setInterval(initializeGoogleSignIn, 500);\n          loadGoogleIdentityLibrary();\n        }\n      }\n      }\n    });\n}\n\nif(document.querySelector('.logout')){\n    document.querySelector('.logout').addEventListener('click', function (event) {\n        if(document.querySelector('.oi-registration-popup')){\n            document.querySelector('.oi-registration-popup').classList.remove('login');\n        }\n        if(document.querySelector('.oi-registration-popup-overlay')){\n            document.querySelector('.oi-registration-popup-overlay').classList.remove('login');\n        }\n        if(document.querySelector('.oi-myprofile-wrapper')){\n            document.querySelector('.oi-myprofile-wrapper').classList.remove('showprofile');\n        }\n        if (document.querySelector('.oi-myprofile-wrapper')) {\n            document.querySelector('.oi-myprofile-wrapper').classList.remove('footerprofile');\n        }\n        if(document.querySelector(\".oi-user\")){\n            document.querySelector(\".oi-user\").classList.remove(\"loggedin\");\n        }\n        if(document.getElementById('profileicon')){\n            document.getElementById('profileicon').style.display = 'inline-block';\n        }\n        if(document.getElementById('profileiconactive')){\n           document.getElementById('profileiconactive').style.display = 'none';\n        }\n        //document.body.style.overflow = \"auto\";\n        logoutUserFromDB(login_id);\n        isLoggedin=0;\n    });\n}\nif (document.querySelector('.oi-user') || document.querySelector('.bottom-oi-user')) {\n    document.querySelectorAll('.oi-user, .bottom-oi-user').forEach(function(element) {\n        element.addEventListener(\"click\", function () {\n            const clickedClass = event.currentTarget.classList.contains('bottom-oi-user') ? 'bottom-oi-user' : 'oi-user';\n            console.log('Clicked class:', clickedClass);\n            if (isLoggedin == 0) {\n                if (document.querySelector('.oi-registration-popup')) {\n                    document.querySelector('.oi-registration-popup').classList.add('login');\n                }\n                if (document.querySelector('.oi-registration-popup-overlay')) {\n                    document.querySelector('.oi-registration-popup-overlay').classList.add('login');\n                }\n                if (document.querySelector('.oi-myprofile-wrapper')) {\n                    document.querySelector('.oi-myprofile-wrapper').classList.remove('showprofile');\n                }\n                if(clickedClass==='bottom-oi-user'){\n                    if (document.querySelector('.oi-myprofile-wrapper')) {\n                        document.querySelector('.oi-myprofile-wrapper').classList.remove('footerprofile');\n                    }\n                }\n            } else {\n                if (document.querySelector('.oi-registration-popup')) {\n                    document.querySelector('.oi-registration-popup').classList.remove('login');\n                }\n                if (document.querySelector('.oi-registration-popup-overlay')) {\n                    document.querySelector('.oi-registration-popup-overlay').classList.add('login');\n                }\n                if(clickedClass==='bottom-oi-user'){\n                    if (document.querySelector('.oi-myprofile-wrapper')) {\n                        document.querySelector('.oi-myprofile-wrapper').classList.add('footerprofile');\n                    }\n                    setTimeout(function(){\n                        if (document.querySelector('.oi-myprofile-wrapper')) {\n                            document.querySelector('.oi-myprofile-wrapper').classList.add('showprofile');\n                        } \n                    },300);\n                }else{\n                   if (document.querySelector('.oi-myprofile-wrapper')) {\n                        document.querySelector('.oi-myprofile-wrapper').classList.add('showprofile');\n                    } \n                }\n            }\n            // document.body.style.overflow = \"hidden\";\n        });\n    });\n}\n\nif(document.querySelector('.oi-login-close')){\ndocument.querySelectorAll(\".oi-login-close, .oi-registration-popup-overlay,.oi-login-overlay\").forEach(function (element) {\n    element.addEventListener(\"click\", function () {\n            if(document.querySelector('.oi-registration-popup')){\n                document.querySelector('.oi-registration-popup').classList.remove('login');\n            }\n            if(document.querySelector('.oi-registration-popup-overlay')){\n                document.querySelector('.oi-registration-popup-overlay').classList.remove('login');\n            }\n            if(document.querySelector('.oi-myprofile-wrapper')){\n                document.querySelector('.oi-myprofile-wrapper').classList.remove('showprofile');\n            }\n            if (document.querySelector('.oi-myprofile-wrapper')) {\n                document.querySelector('.oi-myprofile-wrapper').classList.remove('footerprofile');\n            }\n        //document.body.style.overflow = \"auto\";\n    });\n});\n}\nif(document.querySelector('.google-login')){\ndocument.querySelector(\".google-login\").addEventListener(\"click\", function (e) {\n    e.preventDefault();\n    startGoogleSignIn();\n});\n}\nif(document.querySelector('.oi-myprofile-close')){\ndocument.querySelector(\".oi-myprofile-close\").addEventListener(\"click\", function () {\n    if(document.querySelector('.oi-registration-popup-overlay')){\n                document.querySelector('.oi-registration-popup-overlay').classList.remove('login');\n            }\n            if(document.querySelector('.oi-myprofile-wrapper')){\n                document.querySelector('.oi-myprofile-wrapper').classList.remove('showprofile');\n            }\n            setTimeout(function(){\n                if (document.querySelector('.oi-myprofile-wrapper')) {\n                    document.querySelector('.oi-myprofile-wrapper').classList.remove('footerprofile');\n                }\n            },500);\n            \n    //document.body.style.overflow = \"auto\";\n});\n}\nif(document.querySelector('.oi-myprofile-selectbox')){\n    document.querySelectorAll('.oi-myprofile-selectbox').forEach(function(element) {\n        element.addEventListener('click', function(event) {\n            event.stopPropagation();\n            if (element.classList.contains('show-dropdown')) {\n                document.querySelectorAll('.oi-myprofile-selectbox').forEach(function(el) {\n                    el.classList.remove('show-dropdown');\n                });\n            } else {\n                document.querySelectorAll('.oi-myprofile-selectbox').forEach(function(el) {\n                    el.classList.remove('show-dropdown');\n                });\n                element.classList.add('show-dropdown');\n            }\n        });\n    });\n}\nif(docu